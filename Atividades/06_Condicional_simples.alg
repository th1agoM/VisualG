Algoritmo "condicional_simpes"

Var
salario: real

Inicio
escreval("---------------------------------------")
escreva("Digite o salário desejado: ")
leia(salario)

// CONDICIONAL SIMPLES
se (salario <= 2000) entao
   // BLOCO DE INSTRUÇÕES
   escreva("Candidato selecionado para a primeira fase!")
fimse

Fimalgoritmo