Algoritmo "semnome"

Var
	a, b, c, d, z, t: real

Inicio
			a <- 8 + 14
			b <- 5 - 2
			c <- 4 * 12
			d <- 10 / 2
			z <- 2 ^ 10
			t <- a + b + c + d * z

		escreval("Valor de 'a' : ", a)
		escreval("Valor de 'b' : ", b)
		escreval("Valor de 'c' : ", c)
		escreval("Valor de 'd' : ", d)
		escreval("Valor de 'z' : ", z)
		escreval("Valor de 't' : ", t)

Fimalgoritmo